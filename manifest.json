{
    "manifest_version": 3,
    "name": "Vrau",
    "description": "Cria um bot√£o para download dos arquivos do 'Tarefas' (Funpar).",
    "version": "2.3",
    "content_scripts": [{
        "matches": ["http://200.17.198.2:8089/Tarefa*"],
